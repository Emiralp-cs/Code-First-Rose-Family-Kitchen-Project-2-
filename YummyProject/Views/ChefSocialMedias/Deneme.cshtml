@model List<YummyProject.Models.ChefSocial>


@{
    ViewBag.Title = "Deneme";
    Layout = "~/Views/AdminLayout/Index.cshtml";
    YummyProject.Context.YummyContext context = new YummyProject.Context.YummyContext();
    TempData["deneme"] = TempData["id"];
}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.2/font/bootstrap-icons.min.css">
<table class="table table-bordered table-hover">

    <tr>
        <th>Social</th>
    </tr>


    @foreach (var item in Model)
    {
        <tr>
            <td><i class="@item.Icon"></i></td>
            <td><a href="@item.Url" class="btn btn-outline-secondary">Url</a></td>
            <td><a href="/ChefSocialMedias/DeleteChefSocial/@item.ChefSocialId" class="btn btn-danger">Delete</a></td>
            <td><a href="/ChefSocialMedias/UpdateChefSocials/@item.ChefSocialId" class="btn btn-primary">Update</a></td>
        </tr>
    }




</table>




<a href="/ChefSocialMedias/AddChefSocials/@TempData["id"]" class="btn btn-success">Add New Social</a>





