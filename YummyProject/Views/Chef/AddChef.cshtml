@model YummyProject.Models.Chef
@{
    ViewBag.Title = "AddChef";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Chef Add Page</h2>


<form method="post" action="/Chef/AddChef" enctype="multipart/form-data">

    <label>Upload Image</label>
    <input type="file" name="ImageFile" />
    <br />
    <hr />

    <label>Name</label>
    @Html.TextBoxFor(x => x.ChefName, new { @class = "form-control" })

    <label>Title</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br />
    <hr />

    <label>Description</label>
    @Html.TextAreaFor(x => x.Description, 5, 1, new { @class = "form-control" })


    <hr />



    <button class="btn btn-outline-success">Add</button>






</form>




