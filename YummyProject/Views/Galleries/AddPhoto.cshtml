@model YummyProject.Models.PhotoGallery
@{
    ViewBag.Title = "AddPhoto";
    Layout = "~/Views/AdminLayout/Index.cshtml";

}

<br />
<h3 style="text-align:center">Add New Featured Area Page</h3>

<hr />

<form method="post" enctype="multipart/form-data">

    <label>Upload Image</label>
    <br />
    <input type="file" name="ImageFile" />
    <br />
    @Html.ValidationMessageFor(x => x.ImageUrl, "", new { @class = "text-danger" })
    <hr />

    <div style="text-align:center">
        <button class="btn btn-outline-info">Add</button>
    </div>
    <br />
</form>

